<title>Show uniforms</title>
</head>

<body>
  <%- include('header') -%>
  
    <div class="p-5 col-sm-8 text-dark">
         <h1> <img src="/acsj.png"width="150" height="150"/><%=data[0].school_name%> </h1>

    </div>
  <div class = "center">
  <div class="col-sm-8" style="padding-bottom: 30px;">

    <table class="table table-md table-striped table-hover  table-bordered ">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Type</th>
      <th scope="col">Size</th>
      <th scope="col">Quantity</th>
    </tr>
  </thead>
  <tbody>
         <%for (let i=0; i< data.length; i+=1) {%>
    <tr>
      <th scope="row"><%=i+1%></th>
      <td><%=data[i].type%></td>
      <td><%=data[i].size%></td>
      <td><%=data[i].count%></td>
    </tr>
     <% }; %>
   
  </tbody>
</table>

</div>
</div>
</body>

